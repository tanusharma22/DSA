/**
 * @param {number[]} nums
 * @param {number} val
 * @return {number}
 */
var removeElement = function(nums, val) {
    /**
 * @param {number[]} nums
 * @return {number}
 */
    var n=nums.length
    if(n<=1)
    {
        return n
    }    
    var i=0
    var j=0
    while(i<n)
    {
        if(nums[i]!=val)
        {nums[j]=nums[i]
         j=j+1
        }
        i=i+1
    }
    return(j)
};
